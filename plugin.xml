<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="startTime.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="startTime.commands.category"
            name="Sample Command"
            id="startTime.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="starttime.handlers.SampleHandler"
            commandId="startTime.commands.sampleCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="startTime.commands.sampleCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
         <startup class="starttime.StartUpTimeShow"/>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
   </extension>

</plugin>
